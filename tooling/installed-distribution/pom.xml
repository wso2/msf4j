        <relativePath>../pom.xml</relativePath>
    </parent>

    <artifactId>wso2-developer-studio-eclipse-msf4j-installed-distributions</artifactId>
    <packaging>pom</packaging>
    <name>WSO2 Developer Studio MSF4J Tools Installed Eclipse Distributions</name>
    <repositories>
    	<repository>
    		<id>wso2-maven2-repo</id>
